<div class="row">
<% if logged_in? && current_user.admin? %>
<%= render "shared/navbaradmin"%>
<% else %>
<%= render 'shared/navbaruser' %>
<% end %>



</div>

<div class="row">
<% provide(:title, 'Modifier une Circularisation') %>
<div class="col-md-6 col-md-offset-3">
<h2>Voir votre circularisation</h2>
<hr>
</div>
</div>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for :bankaccount, url: bankaccount_path(@bankaccount), method: :patch do |f| %>
      <%#= render 'shared/error_messages', object: f.object %>

      <%= f.label :bank, "Banque" %>
      <%= f.text_field :bank, class: 'form-control' %>

      <%= f.label :client, "Client" %>
      <%= f.text_field :client, class: 'form-control' %>


<%= f.submit "Retirer ce bouton", class: "btn btn-primary" %>

<% end %>

  </div>
</div>


