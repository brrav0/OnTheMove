 <% if @clientcontact.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
      Le formulaire contient <%= pluralize(@clientcontact.errors.count, "erreurr") %>.
    </div>
    <ul>
    <% @clientcontact.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>
